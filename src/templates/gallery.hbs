{{#each this}}
<li class="item">
    <div class="photo-card">
        <img src="{{previewURL}}" alt="image" />

        <div class="stats">
            <p class="stats-item">
                <i class="material-icons">thumb_up_of </i>
                {{likes}}
            </p>
            <p class="stats-item">
                <i class="material-icons">visibility </i>
                {{views}}
            </p>
            <p class="stats-item">
                <i class="material-icons">comments </i>
                {{comments}}
            </p>
            <p class="stats-item">
                <i class="material-icons">downloads</i>
                {{downloads}}
            </p>
        </div>
    </div>
</li>
{{/each}}